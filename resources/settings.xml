<!-- -*- mode:xml; tab-width:2; c-basic-offset:2; intent-tabs-mode:nil; -*- ex: set tabstop=2 expandtab: -->
<settings>
    <setting propertyKey="@Properties.userGeneralBackgroundColor" title="@Strings.titleGeneralBackgroundColor">
        <settingConfig type="list">
            <listEntry value="0">BLACK</listEntry>
            <listEntry value="1">WHITE</listEntry>
        </settingConfig>
    </setting>

     <setting propertyKey="@Properties.userGeneralOxDisplay" title="@Strings.titleGeneralOxDisplay">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userGeneralVarioDisplay" title="@Strings.titleGeneralVarioDisplay">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userGeneralMapDisplay" title="Map view Display (no effect on No Map devices)">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userGeneralChartDisplay" title="@Strings.titleGeneralChartDisplay">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userGeneralETDisplay" title="@Strings.titleGeneralETDisplay">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userActivityAutoStart" title="@Strings.titleActivityAutoStart">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userNoSetting" title="">
        <settingConfig type="alphaNumeric" readonly="true">
        </settingConfig>
    </setting>

  <group id="Variometer" title="@Strings.titleSettingsVariometer">
    <setting propertyKey="@Properties.userVariometerRange" title="@Strings.titleVariometerRange">
        <settingConfig type="list">
            <listEntry value="0">3.0 m/s</listEntry>
            <listEntry value="1">5.0 m/s</listEntry>
            <listEntry value="2">10.0 m/s</listEntry>
        </settingConfig>
    </setting>

     <setting propertyKey="@Properties.userVariometerSmoothing" title="@Strings.titleVariometerSmoothing">
        <settingConfig type="list">
            <listEntry value="0">Low</listEntry>
            <listEntry value="1">Medium</listEntry>
            <listEntry value="2">High</listEntry>
            <listEntry value="3">Ultra</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.userVariometerAvgTime" title="@Strings.titleVariometerAvgTime">
        <settingConfig type="list">
            <listEntry value="0">current data</listEntry>
            <listEntry value="1">10 s</listEntry>
            <listEntry value="2">20 s</listEntry>
            <listEntry value="3">30 s</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.userVariometerAvgLast" title="T bug avg for last: thermal/n sec">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userVariometerdE" title="@Strings.titleVariometerdE">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userVariometerAutoThermal" title="@Strings.titleVariometerAutoThermal">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userVariometerThermalDetect" title="@Strings.titleVariometerThermalDetect">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userVariometerPlotOrientation" title="@Strings.titleVariometerPlotOrientation">
        <settingConfig type="list">
            <listEntry value="0">North Up</listEntry>
            <listEntry value="1">Heading Up</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.userVariometerPlotRange" title="@Strings.titleVariometerPlotRange">
        <settingConfig type="list">
            <listEntry value="1">1 min</listEntry>
            <listEntry value="2">2 min</listEntry>
            <listEntry value="3">3 min</listEntry>
            <listEntry value="4">4 min</listEntry>
            <listEntry value="5">5 min</listEntry>
        </settingConfig>
    </setting>
  </group>

  <group id="Sounds" title="@Strings.titleSettingsSounds">
    <setting propertyKey="@Properties.userSoundsVariometerTones" title="@Strings.titleSoundsVariometerTones">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userVariometerVibrations" title="@Strings.titleVariometerVibrations">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userMinimumClimb" title="@Strings.titleMinimumClimb">
        <settingConfig type="list">
            <listEntry value="0">0.0 m/s</listEntry>
            <listEntry value="1">0.1 m/s</listEntry>
            <listEntry value="2">0.2 m/s</listEntry>
            <listEntry value="3">0.3 m/s</listEntry>
            <listEntry value="4">0.4 m/s</listEntry>
            <listEntry value="5">0.5 m/s</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.userMinimumSink" title="@Strings.titleMinimumSink">
        <settingConfig type="list">
            <listEntry value="0">-1.0 m/s</listEntry>
            <listEntry value="1">-2.0 m/s</listEntry>
            <listEntry value="2">-3.0 m/s</listEntry>
            <listEntry value="3">-4.0 m/s</listEntry>
            <listEntry value="4">-6.0 m/s</listEntry>
            <listEntry value="5">-10.0 m/s</listEntry>
        </settingConfig>
    </setting>
  </group>

   <!-- <group id="Activity" title="@Strings.titleSettingsActivity">
    <setting propertyKey="@Properties.userActivityAutoStart" title="@Strings.titleActivityAutoStart">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userActivityAutoSpeedStart" title="@Strings.titleActivityAutoSpeedStart">
        <settingConfig type="numeric" errorMessage="error" />
    </setting>
  </group> -->

  <group id="Chart" title="@Strings.titleSettingsChart" enableIfTrue="@Properties.userGeneralChartDisplay">
    <setting propertyKey="@Properties.userChartMinMax" title="@Strings.titleChartMinMax">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userChartValue" title="@Strings.titleChartValue">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userChartShow" title="Show on activity recording start/always">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userChartRange" title="Range last 4hrs/30min">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userChartDisplay" title="@Strings.titleChartDisplay">
        <settingConfig type="list">
            <listEntry value="0">Altitude</listEntry>
            <listEntry value="1">Ascent</listEntry>
            <listEntry value="2">Vertical Speed</listEntry>
            <listEntry value="3">Speed</listEntry>
            <listEntry value="4">Heart Rate</listEntry>
            <listEntry value="5">Acceleration</listEntry>
        </settingConfig>
    </setting>
  </group>

  <group id="Map" title="@Strings.titleSettingsMap" enableIfTrue="@Properties.userGeneralMapDisplay">
    <setting propertyKey="@Properties.userMapHeader" title="@Strings.titleMapHeader">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userMapData" title="@Strings.titleMapData">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userMapTrack" title="@Strings.titleMapTrack">
        <settingConfig type="boolean" />
    </setting>
  </group>

  <group id="Ox" title="@Strings.titleSettingsOx" enableIfTrue="@Properties.userGeneralOxDisplay">
    <setting propertyKey="@Properties.userOxMeasure" title="Enable: over caution elev/always on ">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.userOxElevation" title="O2 Caution Elevation [m]">
        <settingConfig type="numeric" min="0" max="5000" >
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.userOxCritical" title="SpO2 Critical level [%]">
       <settingConfig type="numeric" min="75" max="90" >
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.userOxVibrate" title="@Strings.titleOxVibrate">
        <settingConfig type="boolean" />
    </setting>
  </group>

  <group id="Units" title="@Strings.titleSettingsUnits">
    <setting propertyKey="@Properties.userUnitDistance" title="@Strings.titleUnitDistance">
        <settingConfig type="list">
            <listEntry value="-1">Auto</listEntry>
            <listEntry value="0">km</listEntry>
            <listEntry value="1">sm</listEntry>
            <listEntry value="2">nm</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.userUnitElevation" title="@Strings.titleUnitElevation">
        <settingConfig type="list">
            <listEntry value="-1">Auto</listEntry>
            <listEntry value="0">m</listEntry>
            <listEntry value="1">ft</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.userUnitPressure" title="@Strings.titleUnitPressure">
        <settingConfig type="list">
            <listEntry value="-1">Auto</listEntry>
            <listEntry value="0">mb</listEntry>
            <listEntry value="1">inHg</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.userUnitRateOfTurn" title="@Strings.titleUnitRateOfTurn">
        <settingConfig type="list">
            <listEntry value="0">°/s</listEntry>
            <listEntry value="1">rpm</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.userUnitWindSpeed" title="@Strings.titleUnitWindSpeed">
        <settingConfig type="list">
            <listEntry value="-1">Auto</listEntry>
            <listEntry value="0">kph</listEntry>
            <listEntry value="1">mph</listEntry>
            <listEntry value="2">kt</listEntry>
            <listEntry value="3">m/s</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.userUnitDirection" title="@Strings.titleUnitDirection">
        <settingConfig type="list">
            <listEntry value="0">°</listEntry>
            <listEntry value="1">txt</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.userUnitHeading" title="@Strings.titleUnitHeading">
        <settingConfig type="list">
            <listEntry value="0">°</listEntry>
            <listEntry value="1">txt</listEntry>
        </settingConfig>
    </setting>

    <!-- <setting propertyKey="@Properties.userUnitTimeUTC" title="@Strings.titleUnitTimeUTC">
        <settingConfig type="list">
            <listEntry value="false">Local Time</listEntry>
            <listEntry value="true">UTC</listEntry>
        </settingConfig>
    </setting> -->
  </group>

</settings>
